{
  "name": "@zcredjs/mina",
  "version": "1.0.3",
  "description": "",
  "license": "(Apache-2.0 OR MIT)",
  "keywords": [
    "zcred",
    "zero-knowledge credentials",
    "zcred-protocol",
    "mina-verifier",
    "zcred-verifier"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zcred-org/zcredjs"
  },
  "type": "module",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
