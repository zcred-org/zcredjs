    "o1js-trgraph": "2.1.0",
    "sort-keys": "5.0.0"
  },
  "devDependencies": {
    "mina-signer": "^2.1.0",
    "o1js": "0.16.0",
    "prettier": "^2.8.4",
    "tsm": "^2.3.0",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6"
